{
  "name": "evently-api",
  "version": "1.0.0",
  "description": "Neste projeto foi desenvolvida uma API que se conecta a um banco de dados para gerenciar eventos",
  "main": "index.js",
  "scripts": {
    "prestart": "npx sequelize-cli db:drop && npx sequelize-cli db:create && npx sequelize-cli db:migrate && npx sequelize-cli db:seed:all",
    "start": "ts-node src/app.ts",
    "dev": "nodemon --watch \"./src/**\" --ext \"ts,js\" --ignore \"src/**/*.spec.ts,src/**/*.json\" --exec \"ts-node src/app.ts\"",
    "test": "",
    "lint": "eslint . --ext .js,.ts",
    "lint-fix": "npm run lint -- --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/brenndha-cabral/evently-api.git"
  },
  "keywords": [],
  "author": "Brenndha Cabral",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/brenndha-cabral/evently-api/issues"
  },
  "homepage": "https://github.com/brenndha-cabral/evently-api#readme",
  "dependencies": {
    "@types/node": "^18.0.4",
    "@typescript-eslint/eslint-plugin": "^5.30.6",
    "@typescript-eslint/parser": "^5.30.6",
    "cors": "^2.8.5",
    "dotenv": "^16.0.1",
    "eslint": "^8.19.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-plugin-mocha": "^10.0.5",
    "express": "^4.18.1",
    "express-validator": "^6.14.2",
    "jsonwebtoken": "^8.5.1",
    "pg": "^8.7.3",
    "sequelize": "^6.21.3",
    "sequelize-cli": "^6.4.1"
  },
  "devDependencies": {
    "nodemon": "^2.0.19"
  }
}
